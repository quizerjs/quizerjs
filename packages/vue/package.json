{
  "name": "@quizerjs/vue",
  "version": "0.0.1",
  "description": "quizerjs Vue 3 集成包",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "source": "./src/index.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "lint": "eslint src --ext .ts,.vue",
    "lint:fix": "eslint src --ext .ts,.vue --fix",
    "typecheck": "vue-tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@quizerjs/dsl": "workspace:*",
    "@quizerjs/core": "workspace:*",
    "@quizerjs/quizerjs": "workspace:*"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.0",
    "vue": "^3.4.0",
    "vue-tsc": "^1.8.0"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wsxjs/quizerjs.git",
    "directory": "packages/vue"
  }
}
