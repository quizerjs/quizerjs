{
  "name": "@quizerjs/svelte",
  "version": "0.0.1",
  "description": "quizerjs Svelte 集成包",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "svelte": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "svelte": "./dist/index.js",
      "source": "./src/index.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "lint": "eslint src --ext .ts,.svelte",
    "lint:fix": "eslint src --ext .ts,.svelte --fix",
    "typecheck": "svelte-check --tsconfig ./tsconfig.json",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@quizerjs/dsl": "workspace:*",
    "@quizerjs/core": "workspace:*",
    "@quizerjs/quizerjs": "workspace:*"
  },
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "@editorjs/editorjs": "^2.30.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/node": "^20.10.0",
    "svelte": "^4.2.0",
    "svelte-check": "^3.6.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.0"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quizerjs/quizerjs.git",
    "directory": "packages/svelte"
  }
}
